{% case media.media_type %}
    {% when 'image' %}
        <img src="{{ media | image_url }}" alt="{{ media.preview_image.alt }}" height="240px" width="240px" class="w-full h-full object-cover">
    {% when 'external_video' %}
        {{ media |  external_video_tag }}
    {% else %}0
        
{% endcase %}
